package java_exam;

public class Person {
	void name() {}
	protected void number() {}
	void weight() {}
	
	static void show() {}
	private void secret() {}
}
class Student extends Person {
	public void name() {}
    void number() {} // 오버라이딩이면 protected void number 다 적어줘야 한다
	
    //static void weight() {} // 자식클래스의 정적메서드는 부모클래스의 인스턴스를 재정의 할수없다
	static void show() {}
	private void secret() {}
}
