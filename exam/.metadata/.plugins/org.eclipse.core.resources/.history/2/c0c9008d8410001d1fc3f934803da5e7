package control_exam;

public class ex2 {
	
	public static void main(String[] args) {
		
		// 구구단 출력 
		int line = 0;
		for(int i = 2; i <= 9; i++) {
			for(int j = 1; j <= 9; j++) {
				System.out.print( i + "*" + j +"=" + i * j + " ");
				line++;
			}
			if(line % 10 == 0) {
				System.out.println();
			}
		}
	}
}
