package java_exam;

public class loop {
	
	public static void main(String[] args) {
		
		int a = 0;
		
		while(a < 10) {
			a++;
			System.out.println("a="+a);
		}
		
		int b = 0;
		do {
			// 적어도 한번 { } 내 문장 실행
			b++;
			System.out.println("b=" + b);
		}while(b<10); // do 의 식을 적어도 한번 거치고 나서 b의 값이 10보다 작으면 한번더 계속 반복
		
		
		int[] array = new int[];
		
		
		
		
	}
}
