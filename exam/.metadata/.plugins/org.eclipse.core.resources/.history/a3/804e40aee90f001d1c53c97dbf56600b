package java_exam;

public class Flag {
	
	public static void main(String[] args) {
		
		int hol = 0 , jjak = 0;
		int flag = 0;
		
		for(int i = 0; i <= 10; i++) {
			if(flag == 0) {
				hol = hol + i;
			} else {
				jjak = jjak + i;
				flag = 0;
			}
			System.out.println("홀수의 합 : " + );
		}
	}
}
